{
  "manifest_version":2,
  "name":"wx4work",
  "version":"1.0",
  "description":"weixin for work",
  "icons":{
    "16":"images/icon16.png",
    "48":"images/icon48.png",
    "128":"images/icon128.png"
  },
  "browser_action":{
    "default_icon":{
      "19":"images/icon19.png",
      "38":"images/icon38.png"
    },
    "default_title":"wx4work",
    "default_popup":"index.html"
  },
  "background":{
    "scripts":[
      "js/background.js"
    ]
  },
  "permissions":[
    "<all_urls>"
  ]
}